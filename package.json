    "dependencies": {
        "@minecraft/server": "^1.10.0-beta.1.20.70-preview.24",
        "@minecraft/server-admin": "^1.0.0-beta.1.19.80-stable",
        "@minecraft/server-ui": "^1.2.0-beta.1.20.70-preview.24"
    }
